{
  "name": "sterett-qwik",
  "engines": {
    "node": ">=20.0.0"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "eslintConfig": {
    "extends": [
      "ethang",
      "plugin:qwik/recommended"
    ],
    "rules": {
      "max-lines-per-function": "off",
      "react/jsx-max-depth": "off",
      "react/no-unknown-property": "off",
      "qwik/no-react-props": "off",
      "react/jsx-no-literals": "off"
    }
  },
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint \"src/**/*.ts*\" --fix",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.5.5",
    "@builder.io/qwik-city": "^1.5.5",
    "@builder.io/qwik-react": "0.5.4",
    "@portabletext/types": "^2.0.13",
    "@tailwindcss/typography": "^0.5.13",
    "@types/eslint": "^8.56.10",
    "@types/lodash": "^4.17.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-big-calendar": "^1.8.9",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-ethang": "^14.1.9",
    "eslint-plugin-qwik": "^1.5.5",
    "postcss": "^8.4.38",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.4",
    "typescript": "^5.4.5",
    "undici": "^6.18.2",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.59.0"
  },
  "dependencies": {
    "@ethang/project-builder": "^1.1.39",
    "@ethang/toolbelt": "^2.2.0",
    "@heroicons/react": "^2.1.3",
    "@nextui-org/react": "^2.4.1",
    "@portabletext/react": "^3.1.0",
    "@sanity/client": "^6.19.1",
    "@sanity/image-url": "^1.0.2",
    "framer-motion": "^11.2.10",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "moment": "^2.30.1",
    "react-big-calendar": "^1.12.2",
    "sanity-image": "^0.1.7",
    "tailwind-merge": "^2.3.0",
    "zod": "^3.23.8"
  }
}
