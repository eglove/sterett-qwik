{
  "name": "sterett-qwik",
  "engines": {
    "node": ">=20.0.0"
  },
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "eslintConfig": {
    "extends": [
      "ethang",
      "plugin:qwik/recommended"
    ],
    "rules": {
      "max-lines-per-function": "off",
      "react/jsx-max-depth": "off",
      "react/no-unknown-property": "off",
      "qwik/no-react-props": "off",
      "react/jsx-no-literals": "off"
    }
  },
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint \"src/**/*.ts*\" --fix",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.4.5",
    "@builder.io/qwik-city": "^1.4.5",
    "@builder.io/qwik-react": "0.5.3",
    "@portabletext/types": "^2.0.8",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "^8.56.2",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-big-calendar": "^1.8.8",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-ethang": "^14.0.10",
    "eslint-plugin-qwik": "^1.4.5",
    "postcss": "^8.4.35",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.1",
    "typescript": "^5.3.2",
    "undici": "^6.6.2",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1",
    "wrangler": "^3.28.4"
  },
  "dependencies": {
    "@ethang/project-builder": "^1.1.25",
    "@ethang/toolbelt": "^1.1.0",
    "@heroicons/react": "^2.1.1",
    "@nextui-org/react": "^2.2.9",
    "@portabletext/react": "^3.0.11",
    "@sanity/client": "^6.14.2",
    "@sanity/image-url": "^1.0.2",
    "framer-motion": "^11.0.5",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "moment": "^2.30.1",
    "react-big-calendar": "^1.10.3",
    "sanity-image": "^0.1.7",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4"
  }
}
